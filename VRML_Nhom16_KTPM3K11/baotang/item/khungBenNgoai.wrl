# nền nhà to nhất cả bậc thang đi lên sân
DEF	nen Transform {
	children [
		Shape {
			appearance Appearance {
				texture	ImageTexture {
					url	["..\image\nen_san.jpg"]
				}
			}
			geometry Box {
				size 20 20 0.1
			}
	
		}
	]
	rotation 1 0 0 1.57
}

