Transform {
	children [
		#Lòng Trắng
		DEF wEye Transform {
			children Shape {
				appearance Appearance {
					material Material {
						diffuseColor 1 1 1
					}
				}
				geometry Sphere	{
					radius 0.01
				}
			}
			scale 1.2 0.8 0.2
		}
		#Lòng Đen
		DEF bEye Transform {
			children Shape  {
				appearance Appearance {
					material Material {
						diffuseColor 0 0 0
					}
				}
				geometry Sphere	{
					radius 0.007
				}
			}
			scale 1 1 0.2
			translation	0 0 0.0011
		}
	]
	translation	0.045 0.01 0.112
}	
